Private Sub Command1_Click()
    Dim i As Long
    Dim nC As Long
    Dim nDataCount As Long
    Dim nRetVal As Long
    Dim sTemp As String
    Dim aData() As Double
    Dim aColor() As Long
    Dim grafico As Byte
    
    For grafico = 0 To graficoLojaMeta.Count - 1
    
        If grafico <> 0 Then
            frameNome(grafico).left = frameNome(0).left
            frameNome(grafico).top = frameNome(0).top
        End If
        frameNome(grafico).BackColor = vbBlack
    
        nRetVal = RMC_CreateChartOnDC(graficoLojaMeta(grafico).hDC, ID_RMC1, 0, 0, 160, 160, transparent, _
                                      RMC_CTRLSTYLEIMAGE, False, "", "Tahoma", Default)
        If nRetVal < 0 Then GoTo IsError
    
        nRetVal = RMC_AddRegion(ID_RMC1, 5, 5, -5, -5, "", False)
        If nRetVal < 0 Then GoTo IsError
    
        ReDim aColor(1)
        aColor(0) = Red
        aColor(1) = RGB(25, 25, 25)
    
        
        ReDim aData(1)
        aData(0) = CInt(Int((40 * Rnd()) + 1)) + 60
        aData(1) = 100 - aData(0)
        nRetVal = RMC_AddGridlessSeries(ID_RMC1, 1, aData(0), 2, aColor(0), 2, RMC_DONUT_3D, RMC_FULL, 0, False, RMC_VLABEL_LEGENDONLY, RMC_HATCHBRUSH_OFF, 0)
        
        If nRetVal < 0 Then GoTo IsError
    
        nRetVal = RMC_COText(ID_RMC1, 1, sTemp, 100, 270, 400, 50, RMC_BOX_3D, Moccasin, _
                             Default, 0, RMC_LINE_HORIZONTAL, Maroon, "09C")
        If nRetVal < 0 Then GoTo IsError
     
    
        RMC_AddToolTips ID_RMC1, graficoLojaMeta(grafico).hWnd, 100
        
        nRetVal = RMC_Draw(ID_RMC1)
        
        If nRetVal < 0 Then GoTo IsError
    
    Next grafico
    
    Exit Sub
 
