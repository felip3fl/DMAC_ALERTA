Private Sub Picture1_MouseMove(Button As Integer, Shift As Integer, X As Single, y As Single)
    Dim nRetVal As Long
    Dim tINFO As tRMC_INFO
    
    ' In case we've added ToolTips, show them
    nRetVal = RMC_ShowToolTips(ID_RMC1, X, y)
    
    ' only necessary for the Zoom and Magnifier demos
    RMC_SetMouseClick ID_RMC1, RMC_MOUSEMOVE, X, y, tINFO
    If tINFO.nChartType = RMC_MAGNIFIER_MODE Then ' If in the magnifier demo
        ShowMagnification tINFO ' Show the magnification chart
    End If
End Sub

Private Sub Timer1_Timer()
    Dim i As Long
    Dim nData As Double
    
    If Timer1.Interval = 0 Then Exit Sub
    
    For i = 2 To 20
        RMC_GetData ID_RMC1, 1, 1, i, nData  ' Get data...
        RMC_SetSeriesSingleData ID_RMC1, 1, 1, nData, i - 1 ' ... shift it one position to the left...
    Next
    RMC_SetSeriesSingleData ID_RMC1, 1, 1, RMC_RND(100, 800), 20 ' ... and set a new random value on position 20
    RMC_Draw ID_RMC1     ' redraw the chart
End Sub

Private Sub ShowMagnification(tINFO As tRMC_INFO)
    Dim nFirst&, nLast&, nFirstCol&, nLastCol&, nColIndex&, nOldColIndex&
    Dim nCount&, nFirstHLObject&, y&, i&, z&
    Dim aPointsPerCol() As Long
    Dim tINFO2 As tRMC_INFO
    
    ' Called from Picture1_MouseMove() and Picture1_MouseDown()
    
    If tINFO.nChartType = RMC_MAGNIFIER_MODE Then ' If in magnifier mode (=rectangle gets moved)
        If tINFO.nData1 <> 0 Then     ' If rectangle is over the HighLow series
            nFirst = RMC_GetLowPart(tINFO.nData1)   ' Get first data index, which is covered by the rect
            nLast = RMC_GetHighPart(tINFO.nData1)   ' Get last data index, which is covered by the rect
            RMC_SetSeriesDataRange ID_RMC2, 1, 1, nFirst, nLast
            ' To keep it simple, we could now set the label axis to one column and show the HighLow bars evenly spread
            ' in this column, as it is always done, when using the internal zooming mode (check example "Zoom").
            ' Uncomment the next four lines to see the chart:
            ' RMC_SetLAXTickCount ID_RMC2, 1, 1    ' only one column on the label axis
            ' RMC_SetLAXLabelsRange ID_RMC2, 1, 0, 0 ' show no labels
            ' RMC_Draw ID_RMC2                     ' draw the magnified area chart
            ' Exit Sub
            
            ' But we want to show the HighLow objects in the column(s) of the label axis, where they belong to.
            ' so we must check each HighLow object for it's column index, and we must count, how many objects belong
            ' to each column:
            nCount = ((nLast - nFirst) + 1) / 4  ' How many HighLow objects do we have (one object has 4 data points!)
            ' nFirst is always the first one of the four data points, which build one HighLow object
            nFirstHLObject = Fix(nFirst / 4) + 1    ' So we can calculate, what's the index of the first HighLow object
            For i = 1 To nCount                  ' For each HighLow object
                RMC_GetINFO ID_RMC1, tINFO2, 1, 1, nFirstHLObject + (i - 1) ' Get the tRMC_INFO structure
                nColIndex = tINFO2.nGCol                ' ColumnIndex of the HighLow object
                If nFirstCol = 0 Then nFirstCol = nColIndex ' Mark the first label for the label axis
                If nColIndex > nOldColIndex Then        ' New ColumnIndex?
                    y = y + 1                           ' Incr Column count
                    If y = 1 Then
                        ReDim aPointsPerCol(1 To 1)     ' (Re)Dimension the PointsPerColumn-Array
                    Else
                        ReDim Preserve aPointsPerCol(1 To y)
                    End If
                    nOldColIndex = nColIndex
                End If
                aPointsPerCol(y) = aPointsPerCol(y) + 1 ' Increment the counter for the PointsPerColumn for this Column
            Next
            nLastCol = nColIndex                        ' Mark the last label for the label axis
            RMC_SetLAXTickCount ID_RMC2, 1, y           ' Set the number of columns
            RMC_SetSeriesPPColumnArray ID_RMC2, 1, 1, aPointsPerCol(1), y ' set the PointsPerColumn
            RMC_SetLAXLabelsRange ID_RMC2, 1, nFirstCol, nLastCol ' Set the range for the labels on the label axis
            RMC_Draw ID_RMC2                           ' Draw the magnified area chart
        
        Else
            
            ' Rectangle does not cover the HighLow series, so set data range to zero
            RMC_SetSeriesDataRange ID_RMC2, 1, 1, 0, 0
        End If
        RMC_Draw ID_RMC2
    End If
End Sub


